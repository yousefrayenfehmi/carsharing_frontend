{
  "expo": {
    "name": "FITARIKI",
    "slug": "covoiturage-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fitriqi.png",
    "scheme": "covoiturage-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "6",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Cette application a besoin de votre localisation pour vous suggérer des trajets à proximité.",
        "NSLocationAlwaysUsageDescription": "Cette application a besoin de votre localisation pour vous suggérer des trajets à proximité.",
        "NSPhotoLibraryUsageDescription": "Cette application a besoin d'accéder à vos photos pour définir votre photo de profil.",
        "NSCameraUsageDescription": "Cette application a besoin d'accéder à votre caméra pour prendre une photo de profil.",
        "NSPhotoLibraryAddUsageDescription": "Cette application a besoin de sauvegarder des photos dans votre bibliothèque.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "bundleIdentifier": "com.fitariki.covoiturage"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/fitriqi.png"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "package": "com.fitariki.covoiturage",
      "versionCode": 2,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA",
        "POST_NOTIFICATIONS"
      ],
      "useNextNotificationsApi": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/fitriqi.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fitriqi.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/fitriqi.png",
          "color": "#3B82F6",
          "mode": "production"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {
            "infoPlist": {
              "NSAppTransportSecurity": {
                "NSAllowsArbitraryLoads": true
              }
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6d6546c5-fae9-436f-9370-5145b035358d"
      },
      "googleMapsApiKey": "AIzaSyDOumUf4rWVqfBCtBagNH015oFsm3_oab0"
    }
  }
}